{
  "name": "eslint-config-frontera",
  "version": "2.0.0",
  "description": "ESLint shareable config for Frontera",
  "main": "./src/eslint.js",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/upstars-global/eslint-config-frontera#readme",
  "bugs": {
    "url": "https://github.com/upstars-global/eslint-config-frontera/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/upstars-global/eslint-config-frontera.git"
  },
  "exports": {
    ".": "./src/eslint.js",
    "./vue": "./src/vue.js",
    "./package.json": "./package.json"
  },
  "files": [
    "src"
  ],
  "keywords": [
    "eslintconfig",
    "code",
    "quality",
    "style",
    "lint",
    "linter",
    "eslint",
    "code style"
  ],
  "engines": {
    "node": ">=22.4",
    "npm": ">=10.5.0",
    "yarn": ">=1.22.0"
  },
  "scripts": {
    "lint": "eslint .",
    "inspect:eslint": "npx eslint -c ./src/eslint.js --inspect-config",
    "inspect:vue": "npx eslint -c ./src/vue.js --inspect-config",
    "test": "vitest run"
  },
  "devDependencies": {
    "eslint": "^9.12.0",
    "prettier": "^3.3.3",
    "typescript": "^5.5.4",
    "vite": "^5.4.8",
    "vitest": "^2.1.2"
  },
  "dependencies": {
    "@eslint/js": "^9.12.0",
    "@intlify/eslint-plugin-vue-i18n": "^3.0.0",
    "@stylistic/eslint-plugin-js": "^2.9.0",
    "@types/eslint__js": "^8.42.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n": "^17.10.3",
    "eslint-plugin-vue": "^9.28.0",
    "globals": "^15.11.0",
    "typescript-eslint": "^7.18.0"
  },
  "peerDependencies": {
    "eslint": ">= 9",
    "prettier": ">= 3",
    "typescript": ">= 5"
  }
}
